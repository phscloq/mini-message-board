<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <% messages.forEach(message => {%>
      <div>
        <h1><%= message.message %></h1>
        <p><%= message.author %></p>
      </div>
     
    <%}); %>
    <a href="/new">Add Message</a>
  </body>
</html>
